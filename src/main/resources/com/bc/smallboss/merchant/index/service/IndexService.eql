-- [queryStaffByUserId]
SELECT STAFF_ID
, USER_ID
, NAME
, SEX
, PHOTO
, TAG1
, TAG2
, TAG3
, MOBILE
, BRIEF
FROM STAFF
WHERE USER_ID = ##

-- [querySubscribes]
SELECT SUBSCRIBE_ID
, SUBSCRIBE_TIME
, STAFF_ID
, STAFF_NAME
, STAFF_MOBILE
, STATE
, MEMBER_ID
, MEMBER_NAME
, MEMBER_MOBILE
, TIME_MILLIS "millis"
FROM SUBSCRIBE
WHERE STAFF_ID = ##
ORDER BY STATE DESC, TIME_MILLIS;